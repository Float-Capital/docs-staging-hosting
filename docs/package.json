{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "make-cname-staging": "echo 'stagingdocs.float.capital' >static/CNAME",
    "make-cname-production": "echo 'docs.float.capital' >static/CNAME",
    "switch-to-staging": "git remote set-url origin https://github.com/float-capital/docs-staging.git",
    "switch-to-production": "git remote set-url origin https://github.com/float-capital/docs.git",
    "switch-to-development": "git remote set-url origin https://github.com/float-capital/monorepo.git",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "deploy-to-staging": "yarn make-cname-staging && yarn predeploy && yarn switch-to-staging && ENVIRONMENT=staging yarn deploy && yarn switch-to-development",
    "deploy-to-production": "yarn make-cname-production && yarn predeploy && yarn switch-to-production && ENVIRONMENT=production yarn deploy && yarn switch-to-development"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "docusaurus2-dotenv": "^1.4.0",
    "gh-pages": "^3.1.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
